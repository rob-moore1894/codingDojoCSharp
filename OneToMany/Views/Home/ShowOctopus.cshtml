@model Octopus

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@Model.Name</title>
</head>
<body>
    <h1>@Model.Name</h1>
    <p>Hat: @Model.Hat</p>
    <p>Ink: @Model.Ink_Amount</p>

    <h2>Tentacles:</h2>
    <ul>
        @foreach(Tentacle tent in @Model.Tentacles)
        {
            <li>@tent.Name</li>
        }
    </ul>
</body>
</html>