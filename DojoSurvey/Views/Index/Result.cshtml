@model DojoSurvey.Models.Survey

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dojo Survey Index</title>
    <link rel="stylesheet" href="~/css/style.css">
</head>
<body>
    <div class="wrapper">
        <h1>Submitted Info</h1>
        <div id="display">
            <div class="displayName">
                <h3>Name:</h3>
                <h3>@Model.Name</h3>
            </div>
            <div class="displayLocation">
                <h3>Dojo Location:</h3>
                <h3>@Model.Location</h3>
            </div>
            <div class="displayLanguage">
                <h3>Favorite Language:</h3>
                <h3>@Model.Language</h3>
            </div>
            <div class="displayComment">
                <h3>Comment:</h3>
                <h3>@Model.Comment</h3>
            </div>
        </div>
        <div class="button">
            <a href="/"><button id="back">Go Back</button></a>
        </div>
    </div>
</body>
</html>